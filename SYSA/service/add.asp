		If result & "" <> "" Then
			If values & "" = "" Then
				values = result
			else
				values = values & "," & result
			end if
		end if
		pattern = "<a[^>]+?href=(\""|\')([^\'\""]+)\/(UE\d+\.[A-Za-z0-9]+)\1[^>]*>.+<\/a>"
		values = values & "," & result
		result = RegexReplace(strng, pattern)
		If result & "" <> "" Then
			If values & "" = "" Then
				values = result
			else
				values = values & "," & result
			end if
